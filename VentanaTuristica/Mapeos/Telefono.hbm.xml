<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping namespace="VentanaTuristica.Model" assembly="VentanaTuristica" xmlns="urn:nhibernate-mapping-2.2">
	<class name="Telefono" lazy="true" table="`Telefono`" schema="`dbo`">
		<id name="IdTelefono" access="property" column="`IdTelefono`">
			<generator class="native" />
		</id>
		<property name="CodigoInt" column="`CodigoInt`" />
		<property name="CodigoLoc" column="`CodigoLoc`" />
		<property name="IdContacto" column="`IdContacto`" />
		<property name="IdEmpresa" column="`IdEmpresa`" />
		<property name="Numero" column="`Numero`" />
		<property name="Tipo" column="`Tipo`" length="20" />
	</class>
</hibernate-mapping>
