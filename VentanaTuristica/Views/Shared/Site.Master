                    var suma = eval(div7.offsetWidth) + eval(div8.offsetWidth) + eval(div9.offsetWidth);
                    var total = eval(div10.offsetWidth);
                    var residuo = 0;
                    var ladoIzq = div8;
                    var ladoDer = div9;
                    if (suma > total) {
