                   ladoIzq = div7;
                   ladoDer = div10;
                   if (suma > total) {
                       residuo = suma - total;
                       ladoDer.style.height = eval(ladoDer.offsetHeight + residuo) + 'px';
                   }


                }
                else {	//MSIE
                    ancho = eval(document.body.clientWidth);
                    if (ancho <= 970)
                        ancho = 0
                    else
