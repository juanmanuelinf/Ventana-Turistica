                   ladoDer = div2;
                   if (suma > total) {
                       residuo = ((total - suma) / 2) + 1;
                       ladoDer.offsetWidth = eval(ladoDer.offsetWidth - residuo) + px;
                       ladoIzq.offsetWidth = eval(ladoIzq.offsetWidth - residuo) + px;
                   }
                   suma = eval(div7.offsetHeight);
                   total = eval(div10.offsetHeight);
